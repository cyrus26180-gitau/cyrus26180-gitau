#include<stdio.h>
int main () {
	int age;
	int annual_income;
	
	printf("enter age:\t");
	scanf("%d",&age);
	
	printf("enter your annual_income:\t");
	scanf("%d " ,&annual_income);
	
	if (age >=21 &&annual_income>=21000)
	{
	printf("congraturations you qualify for a loan.\n");
	}
	else {
	printf("unfortunately,we are unable to offer you the loan.");
	}
	return 0;
}