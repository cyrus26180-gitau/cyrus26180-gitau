#include <stdio.h>

int main() {
    int password = 1234;
    int pwd;

    do {
        printf("Enter password: ");
        scanf("%d", &pwd);

        if (pwd == password)
            printf("Access granted!\n");
        else
            printf("Access denied! Try again.\n");

    } while (pwd != password);

    return 0;
}